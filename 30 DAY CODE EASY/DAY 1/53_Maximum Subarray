// Runtime: 0 ms, faster than 100.00% of Java online submissions for Maximum Subarray.
// Memory Usage: 38.7 MB, less than 99.62% of Java online submissions for Maximum Subarray.




class Solution {
    public int maxSubArray(int[] nums) {
        
    int max_sum=nums[0];
    int current_sum=0;
        
        for(int i: nums)
        {
            current_sum+=i;
            
             if(i>current_sum)
            current_sum=i;
            
            max_sum=Math.max(current_sum , max_sum );
            
          
        }
        
        
    return max_sum;
    }
}
