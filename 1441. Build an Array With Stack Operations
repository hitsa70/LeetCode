/*
1441. Build an Array With Stack Operations
Runtime: 0 ms, faster than 100.00% of Java online submissions for Build an Array With Stack Operations.
Memory Usage: 39.1 MB, less than 60.45% of Java online submissions for Build an Array With Stack Operations.
*/



class Solution {
    public List<String> buildArray(int[] target, int n) {
        int counter=1;
        ArrayList<String> list=new ArrayList<>();
        
        for(int i:target)
        {
             while(counter<=i)  
                
                {
                    if(i==counter)
                        list.add("Push");
                    else{
                        list.add("Push");
                        list.add("Pop");
                    }
                    counter++;

                }
            
        }
        
        
        return list;
    }
}
